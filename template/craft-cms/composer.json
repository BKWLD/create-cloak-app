{
    "require": {
        "craftcms/cms": "^4.0.0",
        "craftcms/redactor": "3.0.3",
        "craftcms/webhooks": "3.0.4",
        "doublesecretagency/craft-cpcss": "2.6.0",
        "doublesecretagency/craft-cpjs": "2.6.0",
        "presseddigital/colorit": "4.0.0",
        "putyourlightson/craft-dashboard-begone": "2.0.0",
        "spicyweb/craft-neo": "3.6.2",
        "vaersaagod/dospaces": "2.0.0",
        "verbb/super-table": "3.0.7",
        "vlucas/phpdotenv": "^5.4.0"
    },
    "require-dev": {
        "craftcms/generator": "^1.0.0",
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
