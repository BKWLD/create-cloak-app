{
	"name": "<%= rootNuxtApp ? packageName : 'nuxt-app' %>",
	"author": "Bukwild",
	"version": "0.0.1",
	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"dev": "yarn transpile && nuxt",
		"build": "yarn transpile && nuxt build",
		"start": "nuxt start",
		"generate": "yarn transpile && nuxt generate --fail-on-error --target=static",
		<%_ if (cms == 'craft') { _%>
		"transpile": "yarn transpile:coffee && yarn transpile:gql",
		"transpile:gql": "yarn webpack --config <%= rootNuxtApp ? '' : '../' %>node_modules/@bkwld/cloak/build/transpile-gql.coffee",
		<%_ } else { _%>
		"transpile": "yarn transpile:coffee",
		<%_ } _%>
		"transpile:coffee": "coffee -c nuxt.config.coffee",
		"transpile:watch": "coffee -cw nuxt.config.coffee"
	},
	"dependencies": {
		"@bkwld/cloak": "^1.5.1",
		"@bkwld/light-or-dark": "^1.0.1",
		"nuxt": "^2.15.7",
		"vue-balance-text": "^1.2.3",
		"vue-detachable-header": "^0.2.0",
		"vue-unorphan": "^1.2.3"
	}
}
